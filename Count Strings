#include <assert.h>
#include <ctype.h>
#include <limits.h>
#include <math.h>
#include <stdbool.h>
#include <stddef.h>
#include <stdint.h>
#include <stdio.h>
#include <stdlib.h>
#include <string.h>



int str_c() {
char s[500];
int a=0,b=0,c=0;
scanf("%[^\n]%*c",s);
for (int i=0;s[i]!='\0';i++)
{if (s[i]==' ')
{
a++;
}
else b++;
 }   
 c=a+b;
return c;
}

int main()
{ char s[500];
    
    int count = str_c(s);

    printf("%d",count);

    return 0;
}
