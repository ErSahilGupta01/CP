#include<bits/stdc++.h>
using namespace std;
int main()
{
int a, b,n,T,r,i,j,k,fact,facts,f,s;
    scanf("%d",&T);
    
    while(T--)
    { fact=1;
        facts=1;
     f=1;
        scanf("%d %d",&n,&r);
        a=n-r;
        for (i=1;i<=n;i++)
        {
            fact=fact*i;
        }
          for (j=1;j<=r;j++)     
          {
              facts=facts*j;
          }
    for (k=1;k<=a;k++)
    {
        f=f*k;
    }
    s=f*facts;
    b=fact/s;
    printf("%d\n",b);
    }
    return 0;
 }
