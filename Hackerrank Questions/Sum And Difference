#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main()
{
int diff, a,b,sum=0;
float dif,x,y,sume=0.0;
scanf("%d %d\n",&a,&b);
scanf("%f %f\n",&x,&y);
sum=a+b;
diff=a-b;
sume=x+y;
dif=x-y;
printf("%d %d\n",sum,diff );
printf("%0.1f %0.1f\n",sume,dif);
    
    return 0;
}
