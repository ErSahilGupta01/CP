#include <bits/stdc++.h>
using namespace std;
#define ll long long
int main()
{
    ll t;cin>>t;
    while(t--)
    {
        ll n,sum;
        cin>>n>>sum;
        ll a[n];
        for(ll i=0;i<n;i++)
        {
            cin>>a[i];
        }
        for(ll i=0;i<n;i++)
        {
            if(sum-a[i]>=0)
            {
                cout<<"1";
                  sum=sum-a[i];
            }
            else
            cout<<"0";
          
        }
        cout<<endl;
    }

    return 0;
}
