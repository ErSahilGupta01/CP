#include <bits/stdc++.h>
using namespace std;
#define ll long long
int main()
{   ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    ll t;
    cin>>t;
    while(t--)
{  ll n,c=0;
   cin>>n;
   ll a[4];
   for(ll i=0;i<4;i++)
   {
       cin>>a[i];
   }
   for(ll i=0;i<4;i++)
   {
       c=max(c,a[i]);
   }
   cout<<c<<endl;
}
    return 0;
}
