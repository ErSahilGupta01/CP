#include <bits/stdc++.h>
using namespace std;
#define ll long long
int main()
{  
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    ll t;cin>>t;
    while(t--)
    {
        ll n,s;
        cin>>n>>s;
        if(s==0)
        {
            if(n%4==0)
            {
                cout<<"Off"<<endl;
            }
            else 
            cout<<"On"<<endl;
        }
        else
        {
            if(n==0||n%4==0)
            {
                cout<<"On"<<endl;
            }
            else
            {
                cout<<"Ambiguous"<<endl;
            }
        }
    }
    
    return 0;
}
