#include <bits/stdc++.h>
using namespace std;
#define ll long long
int main()
{
    ll t;
    cin>>t;
    while(t--)
{  ll n;
   cin>>n;
   ll a[n];
   for(ll i=0;i<n;i++)
   {
       cin>>a[i];
   }
   for(ll i=n-1;i>=0;i--)
   {
       if(a[i]!=0)
       {
           cout<<i<<endl;
           break;
       }
       
   }
}
    return 0;
}
