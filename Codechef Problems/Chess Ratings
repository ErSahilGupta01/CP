#include <bits/stdc++.h>

using namespace std;
#define ll long long
int main()
{  ll t;cin>>t;
while(t--)
{
   ll a,b,c=0,d;
   cin>>a>>b;
   d=a;
   for(ll i=0;i<10000;i++)
   {
      if(d>=b)
      {
          cout<<c<<endl;
          break;
      }
      else
      {
          d+=8;
          c+=1;
      }
   }
}
    return 0;
}
