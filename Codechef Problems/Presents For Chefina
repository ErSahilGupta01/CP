#include <bits/stdc++.h>
using namespace std;
#define ll long long
int main()
{  
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
   ll t;
   cin>>t;
   
   while(t--)
   { 
     ll n,a=0;
     cin>>n;
     if(n%4==0)
     {
         a=(n/5);
     }
     else
     {
         a=n/5;
     }
     cout<<n-a<<endl;
   }

    return 0;
}
