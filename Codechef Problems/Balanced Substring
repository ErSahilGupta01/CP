#include <bits/stdc++.h>
using namespace std;
int main()
{  
    int T;
    cin>>T;
    for(int i=0;i<T;i++)
    {
        int n;
        cin>>n;
        string s;
        cin>>s;
        for(int i=0;i<n-1;i++)
        {
            if(s[i]!=s[i+1])
            {
                cout<<i+1<<i+2<<endl;
                break;
            }
        }    
        if(n==1)
        cout<<-1<<-1<<endl;
        
    }
}
