#include <bits/stdc++.h>
using namespace std;
#define ll long long
int main()
{  
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    ll t;cin>>t;
    while(t--)
    {
        ll n,a;
        cin>>n>>a;
        if(a>=n&&a%n==0)
        {
            cout<<"YES"<<endl;
        }
        else cout<<"NO"<<endl;
    }
    
    return 0;
}
